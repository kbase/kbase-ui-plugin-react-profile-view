{
    "name": "ant-design-profile",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "@craco/craco": "5.3.0",
        "@kbase/ui-lib": "0.1.0-alpha.50",
        "@types/enzyme": "3.10.3",
        "@types/enzyme-adapter-react-16": "1.0.5",
        "@types/jest": "24.0.18",
        "@types/node": "12.7.2",
        "@types/react": "16.9.2",
        "@types/react-dom": "16.9.0",
        "@types/react-redux": "7.1.2",
        "antd": "3.22.1",
        "craco-antd": "1.11.0",
        "enzyme": "3.10.0",
        "enzyme-adapter-react-16": "1.14.0",
        "react": "16.9.0",
        "react-dom": "16.9.0",
        "react-redux": "7.1.1",
        "react-scripts": "3.1.1",
        "react-test-renderer": "16.9.0",
        "redux": "4.0.4",
        "typescript": "3.5.3"
    },
    "scripts": {
        "start": "nodemon -w ./craco.config.js -w ./src/custom/style/antd/theme.less --exec 'craco start'",
        "build": "craco build",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "typedoc": "typedoc --exclude \"**/*.test.*\" --out spec src",
        "install-plugin": "bash scripts/install-plugin.bash"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "proxy": "https://ci.kbase.us",
    "devDependencies": {
        "nodemon": "^1.19.1",
        "typedoc": "0.15.0"
    },
    "jest": {
        "collectCoverageFrom": [
            "./src/**/*.{ts,tsx}",
            "src/components/Narratives.tsx",
            "src/components/SearchUsers.tsx",
            "src/components/Profile.tsx",
            "!/node_modules/",
            "!src/index.tsx",
            "!src/serviceWorker.ts"
        ]
    },
    "homepage": "./"
}
