    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "proxy": "https://ci.kbase.us",
    "devDependencies": {
        "@testing-library/dom": "7.21.7",
        "@testing-library/jest-dom": "5.11.2",
        "@testing-library/react": "10.4.7",
        "@testing-library/user-event": "12.1.0",
        "nodemon": "2.0.4",
        "typedoc": "0.17.8"
    },
    "jest": {
        "collectCoverageFrom": [
            "./src/**/*.{ts,tsx}",
            "src/components/Narratives.tsx",
            "src/components/SearchUsers.tsx",
            "src/components/Profile.tsx",
            "!/node_modules/",
            "!src/index.tsx",
            "!src/serviceWorker.ts"
        ]
    },
    "homepage": "./"
