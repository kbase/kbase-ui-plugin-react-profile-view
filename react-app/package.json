        "@kbase/ui-lib": "0.2.1-alpha.3",
        "@types/enzyme": "*",
        "@types/enzyme-adapter-react-16": "*",
        "@types/jest": "*",
        "@types/marked": "*",
        "@types/node": "*",
        "@types/react": "*",
        "@types/react-dom": "*",
        "@types/react-redux": "*",
        "antd": "4.5.2",
        "babel-plugin-import": "1.13.0",
        "customize-cra": "1.0.0",
        "enzyme": "3.11.0",
        "enzyme-adapter-react-16": "1.15.2",
        "less": "3.12.2",
        "less-loader": "6.2.0",
        "marked": "2.0.0",
        "react": "16.13.1",
        "react-app-rewired": "2.1.6",
